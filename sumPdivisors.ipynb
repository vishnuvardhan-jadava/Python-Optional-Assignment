      "source": [
        "def sumPdivisors(n):\n",
        "  li=[]\n",
        "  for i in range(1,n):\n",
        "    if (n % i) ==0:\n",
        "      li.append(i)\n",
        "  print('divisiors of {} are: {}'.format(n,li))\n",
        "  return sum(li)\n",
        "\n",
        "print(sumPdivisors(int(25)))"
      ],
